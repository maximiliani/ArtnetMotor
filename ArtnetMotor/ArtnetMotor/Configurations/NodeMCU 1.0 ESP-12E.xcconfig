// Board identifier
// See Boards.txt for <tag>.name=Arduino Uno (16 MHz)
//
BOARD_TAG  = nodemcuv2

// For Arduino 1.5.x, if different from Arduino 1.0.x
//
//BOARD_TAG1 = nodemcuv2.menu.FlashSize.4M3M
BOARD_TAG1 = nodemcuv2.menu.eesz.4M3M
BOARD_TAG2 = nodemcuv2.menu.ip.lm2f // v2 Lower Memory
////BOARD_TAG2 = generic.menu.FlashFreq.40
//BOARD_TAG3 = nodemcuv2.menu.CpuFrequency.80
BOARD_TAG3 = nodemcuv2.menu.xtal.80
////BOARD_TAG4 = generic.menu.FlashMode.dio
//BOARD_TAG5 = nodemcuv2.menu.UploadSpeed.115200
BOARD_TAG5 = nodemcuv2.menu.baud.115200

// Port (optional)
// most common are /dev/tty.usbserial*, /dev/tty.usbmodem* or /dev/tty.uart*
//
BOARD_PORT = /dev/cu.SLAB_USBtoUART

// References for Xcode code-sense
// See Boards.txt for <tag>.build.mcu=<GCC_PREPROCESSOR_DEFINITIONS>
//
GCC_PREPROCESSOR_DEFINITIONS = ESP8266 ARDUINO 

// Specify the full path and name of the application
// with /Contents/Java/** after
//
HEADER_SEARCH_PATHS = $HOME/Library/Arduino15/packages/esp8266/hardware/esp8266/**

// Maximum RAM size in bytes
// given by <tag>.upload.maximum_ram_size in boards.txt for Maple and Teensy
// given by <tag>.upload.maximum_data_size in boards.txt for Arduino 1.5.x
//
//MAX_RAM_SIZE = 2048

RESET_MESSAGE = 1

CONFIG_NAME = NodeMCU 1.0 ESP-12E
